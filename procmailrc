SHELL=/bin/bash
VERBOSE=off
MAILDIR=$HOME/incoming/
LOGFILE=$HOME/incoming/.procmaillog

:0
`formail -xSubject: | tr '\n' ' ' | sed -e '{ s/Subject: //; s@\[@@g; s@\]@@g; s@[()]@_@g;
s@[/:]@-@g; s@"@_@g; s@^[[:space:]]\+@@; s@\.\.@.@g; s@[[:space:]]\+@_@g; s@-_@_@g; s@__@_@g;
s@\.$@@; s@_$@@; }'`.mbox
